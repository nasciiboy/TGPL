#            for px := 0; px < width; px++ {
#              x := float64(px)/width*(xmax-xmin) + xmin
#              z := complex(x, y)
#              // Image point (px, py) represents complex value z.
#              img.Set(px, py, mandelbrot(z))
#            }
#          }
